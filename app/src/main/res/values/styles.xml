<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>



    </style>

    <style name="titlestyle">

        <item name ="android:layout_width">match_parent</item>
        <item name ="android:layout_height">20dp</item>
        <item name ="android:background">@color/colorPrimary</item>

    </style>
    <style name="spinner1">

        <item name ="android:layout_width">100dp</item>
        <item name ="android:layout_height">100dp</item>
        <item name ="android:entries">@array/car_brand</item>
        <item name ="android:prompt">@string/brand_prompt</item>
        <item name ="android:background">@color/spiner</item>


    </style>
    <style name="spinner2">

        <item name ="android:layout_width">100dp</item>
        <item name ="android:layout_height">100dp</item>
        <item name ="android:entries">@array/car_model</item>
        <item name ="android:spinnerMode">dialog</item>
        <item name ="android:splitMotionEvents">true</item>
        <item name ="android:background">@color/spiner</item>

    </style>

    <style name="edittxt1">

        <item name ="android:layout_width">match_parent</item>
        <item name ="android:layout_height">69dp</item>
        <item name ="android:hint">palets</item>
        >


    </style>
    <style name="edittxt2">

        <item name ="android:layout_width">match_parent</item>
        <item name ="android:layout_height">69dp</item>
        <item name ="android:hint">color</item>
        >
    </style>

    <style name="Button">

        <item name ="android:layout_width">53dp</item>
